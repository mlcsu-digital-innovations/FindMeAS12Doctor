{
  "name": "fmas12d",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "del-cli dist && ng build",
    "build:aimes-uat": "del-cli ../Deployment/AimesUat/Fmas12d.Ui --force && ng build --configuration=aimes-uat && cpy ./deployment/web.config ../Deployment/AimesUat/Fmas12d.Ui",
    "build:azure-development": "del-cli ../Deployment/AzureDevelopment/Fmas12d.Ui --force && ng build --configuration=azure-development && cpy ./deployment/web.config ../Deployment/AzureDevelopment/Fmas12d.Ui",
    "build:prod": "del-cli dist && ng build --prod --build-optimizer",
    "test": "ng test",
    "test-coverage": "del-cli .testing && ng test --code-coverage",
    "test-headless": "del-cli .testing && ng test --code-coverage --karma-config ./src/karma-headless.conf.js",
    "test-vsts": "del-cli .testing && ng test --code-coverage --karma-config ./src/karma-vsts.conf.js",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.4",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/forms": "~8.2.4",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/router": "~8.2.4",
    "@ng-bootstrap/ng-bootstrap": "^5.1.0",
    "@uiowa/digit-only": "^1.1.1",
    "angular-auth-oidc-client": "^10.0.10",
    "exceljs": "^1.12.0",
    "file-saver": "^2.0.2",
    "moment": "^2.24.0",
    "ng-multiselect-dropdown": "^0.2.5",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "xlsx": "^0.15.5",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.2",
    "@angular/cli": "~8.3.2",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "cpy-cli": "^2.0.0",
    "del-cli": "^3.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "serialize-javascript": ">=2.1.1",
    "terser-webpack-plugin": "^1.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "resolutions": {
    "serialize-javascript": "^2.1.1",
    "tree-kill": "^1.2.2"
  }
}
